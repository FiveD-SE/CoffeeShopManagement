{
    "expo": {
        "name": "CoffeeShopManagement",
        "slug": "CoffeeShopManagement",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splashScreenImage/Logowithbrandname.jpg",
            "resizeMode": "contain",
            "backgroundColor": "#887150"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.fived.coffee"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": ["expo-font"],
        "extra": {
            "eas": {
                "projectId": "18f4b2ac-5a6b-471d-84cd-a5a5e0fda9e2"
            }
        }
    }
}
